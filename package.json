{
  "name": "nu-todo-api",
  "version": "1.0.0",
  "description": "Back end for n√ºTodo.",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsc-watch --onSuccess \"npm start\"",
    "start": "node ./dist/server.js",
    "test": "jest --watch"
  },
  "keywords": [
    "node",
    "api",
    "typescript"
  ],
  "author": "Daniel Cornock",
  "license": "ISC",
  "dependencies": {
    "@danielc7150/express-utils": "^3.1.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "pg": "^7.18.2",
    "tsc-watch": "^4.1.0"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.2",
    "@types/jest": "^25.1.3",
    "@types/node": "^13.7.4",
    "@types/pg": "^7.14.1",
    "jest": "^25.1.0",
    "jest-config": "^25.1.0",
    "nodemon": "^2.0.2",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.6.2",
    "tslint": "^6.0.0",
    "typescript": "^3.7.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "json",
      "ts"
    ],
    "rootDir": "src",
    "modulePaths": [
      "<rootDir>"
    ],
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coveragePathIgnorePatterns": [
      "factories"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  },
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=6.0.0"
  }
}
